package basic;
import java.util.Scanner;
public class Leap {

	public static void main(String[] args) {
		int a;
		Scanner scan=new Scanner(System.in);
		a=scan.nextInt();
		if(a%4==0) {
			System.out.println("Leap year");
		}
		else {
			System.out.println("not a Leap year");
		}
		scan.close();
	}

}
